{
  "name": "OKSolar Purple Theme",
  "author": "dieghernan",
  "semanticHighlighting": true,
  "type": "dark",
  "tokenColors": [
    {
      "settings": {
        "background": "#331E34",
        "foreground": "#A1A59A"
      }
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#F23749",
        "fontStyle": "bold"
      }
    },
    {
      "name": "atrule",
      "scope": [
        "entity.name.tag.yaml",
        "keyword.control.at-rule",
        "meta.at-rule",
        "support.constant.media.css"
      ],
      "settings": {
        "foreground": "#819500"
      }
    },
    {
      "name": "attr-name",
      "scope": "entity.other.attribute-name.html",
      "settings": {
        "foreground": "#819500"
      }
    },
    {
      "name": "attr-value",
      "scope": [
        "string.quoted.double.html",
        "string.quoted.single.html"
      ],
      "settings": {
        "foreground": "#259D94"
      }
    },
    {
      "name": "bold",
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "boolean",
      "scope": [
        "constant.language",
        "constant.language.boolean"
      ],
      "settings": {
        "foreground": "#259D94"
      }
    },
    {
      "name": "class",
      "scope": "entity.other.attribute-name.class.css",
      "settings": {
        "foreground": "#2B90D8"
      }
    },
    {
      "name": "class-name",
      "scope": "entity.name.type.class",
      "settings": {
        "foreground": "#2B90D8"
      }
    },
    {
      "name": "code",
      "scope": [
        "text.html.markdown markup.inline.raw",
        "text.html.markdown markup.raw"
      ],
      "settings": {
        "foreground": "#2B90D8"
      }
    },
    {
      "name": "comment",
      "scope": "comment",
      "settings": {
        "foreground": "#766776"
      }
    },
    {
      "name": "content",
      "scope": "meta.embedded.line",
      "settings": {
        "foreground": "#D56500"
      }
    },
    {
      "name": "constant",
      "scope": "variable.other.constant",
      "settings": {
        "foreground": "#259D94"
      }
    },
    {
      "name": "doctype",
      "scope": [
        "meta.tag.metadata.doctype",
        "meta.tag.sgml.doctype.html",
        "meta.tag.sgml.html"
      ],
      "settings": {
        "foreground": "#766776"
      }
    },
    {
      "name": "function",
      "scope": [
        "entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#2B90D8"
      }
    },
    {
      "name": "important",
      "scope": [
        "constant.numeric.yaml-version",
        "entity.other.document",
        "keyword.other.directive",
        "keyword.other.important",
        "meta.directives",
        "punctuation.definition.alias",
        "punctuation.definition.anchor",
        "punctuation.section.embedded",
        "variable.other.alias",
        "variable.other.anchor"
      ],
      "settings": {
        "foreground": "#819500",
        "fontStyle": "bold"
      }
    },
    {
      "name": "italic",
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "keyword",
      "scope": [
        "constant.language.undefined",
        "keyword",
        "markup.inline.raw",
        "markup.raw",
        "storage"
      ],
      "settings": {
        "foreground": "#819500"
      }
    },
    {
      "name": "keyword-null",
      "scope": "constant.language.null",
      "settings": {
        "foreground": "#259D94"
      }
    },
    {
      "name": "namespace,",
      "scope": [
        "entity.name.type.namespace",
        "storage.modifier.import"
      ],
      "settings": {
        "foreground": "#2B90D8"
      }
    },
    {
      "name": "number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#259D94"
      }
    },
    {
      "name": "operator",
      "scope": [
        "keyword.operator.arithmetic",
        "keyword.operator.assignment",
        "keyword.operator.class",
        "keyword.operator.comparison",
        "keyword.operator.logical",
        "keyword.operator.relational",
        "keyword.other",
        "punctuation.separator.dictionary.key-value"
      ],
      "settings": {
        "foreground": "#819500"
      }
    },
    {
      "name": "property",
      "scope": [
        "meta.property-list",
        "support.type.property-name",
        "support.type.property-name.css",
        "support.type.vendored.property-name",
        "variable.other.property"
      ],
      "settings": {
        "foreground": "#AC8300"
      }
    },
    {
      "name": "punctuation",
      "scope": [
        "punctuation.definition.tag",
        "punctuation.separator"
      ],
      "settings": {
        "foreground": "#A1A59A"
      }
    },
    {
      "name": "regex",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#259D94"
      }
    },
    {
      "name": "selector",
      "scope": [
        "meta.selector",
        "source.css entity.name.tag",
        "source.css entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#819500"
      }
    },
    {
      "name": "string",
      "scope": "string",
      "settings": {
        "foreground": "#259D94"
      }
    },
    {
      "name": "tag",
      "scope": [
        "meta.document.yaml",
        "meta.tag",
        "storage.type.tag"
      ],
      "settings": {
        "foreground": "#2B90D8"
      }
    },
    {
      "name": "title",
      "scope": "markup.heading",
      "settings": {
        "foreground": "#AC8300",
        "fontStyle": "bold"
      }
    },
    {
      "name": "variable",
      "scope": "variable.other.object",
      "settings": {
        "foreground": "#A1A59A"
      }
    },
    {
      "name": "url",
      "scope": "markup.underline.link",
      "settings": {
        "foreground": "#2B90D8"
      }
    },
    {
      "name": "CSS text",
      "scope": [
        "meta.property-list.css",
        "source.css keyword.other"
      ],
      "settings": {
        "foreground": "#2B90D8"
      }
    },
    {
      "name": "SCSS variable",
      "scope": "source.css variable",
      "settings": {
        "foreground": "#D56500"
      }
    }
  ],
  "colors": {
